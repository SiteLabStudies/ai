  const Jongno_gu = new google.maps.Polygon({
    paths: Jongno,
    strokeColor: "#000000",
    strokeOpacity: 0.4,
    strokeWeigh: 3,
    fillColor: "#004DA2",
    fillOpacity: 0.8,
  });
  Jongno_gu.setMap(map);
  Jongno_gu.addListener("click", showArrays_04);
  infoWindow = new google.maps.InfoWindow();

  function showArrays_04(event) {

    const polygon = this;
    const vertices = polygon.getPath();
    let contentString =
      "<font size=4><b> 종로구 </b><br></font size><br>" +
      "<b><font size=2>직주비: </b><br>" +
      "3.5221<br>" +
      "<b>주간지수: </b><br>" +
      "244.7</font size>"
      ;
  
    infoWindow.setContent(contentString);
    infoWindow.setPosition(event.latLng);
    infoWindow.open(map);
  }

