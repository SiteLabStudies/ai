  infoWindow = new google.maps.InfoWindow();
  function showArrays_01(event) {

    //const polygon = this;
    let contentString =
      "<font size=4><b> 강남구 </b><br></font size><br>" +
      "<b><font size=2>직주비: </b><br>" +
      "2.9996<br>" +
      "<b>주간지수: </b><br>" +
      "192.7</font size>"
      ;
    infoWindow.setContent(contentString);
    infoWindow.setPosition(event.latLng);
    infoWindow.open(map);
  }
  


